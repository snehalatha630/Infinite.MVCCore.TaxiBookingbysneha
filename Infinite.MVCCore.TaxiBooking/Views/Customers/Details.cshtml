@model CustomerViewModel
@{
    ViewData["Title"] = "Details";
}

<h1>List of Details</h1>

<div class="row">
    <div class="col-md-5">
        <ul class="list-group">
            <li class="list-group-item"><b>Customer Name:</b>@Model.CustomerName</li>
            <li class="list-group-item"><b>Phone Number:</b>@Model.PhoneNo</li>
            <li class="list-group-item"><b>Email Id:</b>@Model.EmailId</li>
            <li class="list-group-item"><b>Address:</b>@Model.Address</li>
            
        </ul>
        <a asp-controller="Customers" asp-action="Index" Back To Index></a>
    </div>
</div>

